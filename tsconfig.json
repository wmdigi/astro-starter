{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@assets/*": ["src/assets/*"],
      "@styles/*": ["src/styles/*"],
      "@images/*": ["src/assets/images/*"],
      "@videos/*": ["src/assets/videos/*"],
      "@icons/*": ["src/components/icons/*"],
      "@lib/*": ["src/js/*"],
      "@src/*": ["src/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}
